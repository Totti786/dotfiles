{
    "wallpaper": "{{image}}",
    "alpha": "100",

    "special": {
        "background": "{{colors.surface_dim.default.hex}}",
        "foreground": "{{colors.on_surface.default.hex}}",
        "cursor": "{{colors.on_surface.default.hex}}"
    },
    "colors": {
        "color0": "{{colors.surface_dim.default.hex}}",
        "color1": "{{colors.primary_fixed_dim.default.hex}}",
        "color2": "{{colors.tertiary.default.hex}}",
        "color3": "{{colors.inverse_primary.default.hex}}",
        "color4": "{{colors.primary.default.hex}}",
        "color5": "{{colors.tertiary.default.hex}}",
        "color6": "{{colors.tertiary_container.default.hex}}",
        "color7": "{{colors.on_surface.default.hex}}",
        "color8": "{{colors.surface_variant.default.hex | set_lightness: -15.0 }}",
        "color9": "{{colors.primary_fixed.default.hex}}",
        "color10": "{{colors.tertiary_fixed.default.hex}}",
        "color11": "{{colors.tertiary_fixed.default.hex}}",
        "color12": "{{colors.primary.default.hex | set_lightness: 0.5 }}",
        "color13": "{{colors.secondary_fixed.default.hex}}",
        "color14": "{{colors.tertiary_container.default.hex | set_lightness: 5.0}}",
        "color15": "{{colors.on_surface.default.hex | set_lightness: 5.0 }}"
    }
}
